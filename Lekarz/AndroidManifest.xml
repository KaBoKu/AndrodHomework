<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lekarz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/doctorimage"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.lekarz.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity 
            android:name=".MainActivity" 
            android:label="@string/title_first_activity">
        </activity>
        
        <activity 
            android:name=".DataActivity" 
            android:label="@string/title_second_activity">
        </activity>
        
         <activity 
            android:name=".CommentActivity" 
            android:label="@string/title_third_activity">
        </activity>
        
          <activity 
            android:name=".EndActivity" 
            android:label="@string/title_fourth_activity">
        </activity>
        
           <activity 
            android:name=".DoctorViewActivity" 
            android:label="@string/title_fivth_activity">
        </activity>
          
          <activity 
            android:name=".GalleryActivity" 
            android:noHistory="true"
            android:label="@string/title_sixth_activity">
        </activity>
           
    </application>

</manifest>